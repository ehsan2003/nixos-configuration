configuration {
    modi: "combi,window,drun,run";
    combi-modi: "window,drun,run";
    icon-theme: "Papirus";
    show-icons: true;
    display-combi: "Applications";
    drun-display-format: "{icon} {name}";
    window-format: "{icon} {title}";
    matching: "fuzzy";
    case-sensitive: false;
    sort: true;
    sorting-method: "fzf";
    threads: 0;
    scroll-method: 1;
    window-thumbnail: true;
    terminal: "alacritty";
}

@theme "theme.rasi"
